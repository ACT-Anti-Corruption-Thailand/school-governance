{
	"name": "school-governance",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"d-extract": "tar xzvf data/csv.tar.gz --directory data/",
		"d-tojson": "python3 data/01-csvtojson.py",
		"d-transpile": "node data/02-transpile.js",
		"d-genschoolindex": "node data/generateSchoolIndex.js",
		"convert": "yarn d-extract && yarn d-tojson && yarn d-transpile"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.48",
		"@sveltejs/kit": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"csvtojson": "^2.0.10",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.56.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@beyonk/svelte-google-analytics": "^2.6.4",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"firebase": "^9.14.0",
		"lottie-svelte": "^1.3.3",
		"motion": "^10.14.3",
		"svelte-action-balancer": "^1.0.4"
	}
}
